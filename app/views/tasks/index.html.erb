<div class="container">
<h1>My tasks</h1><ul>
<% @tasks.each do |task| %>
<li><%= task.completed %> <%= link_to task.title, task_path(task) %><%= link_to image_tag('https://static.thenounproject.com/png/888071-200.png', width: 20), destroy_task_path(task), method: 'delete', data: { confirm: 'Are you sure you want to delete?' }%></li>
<% end %>
</ul>
<%= link_to 'Create a task', new_task_path %>
</div>
